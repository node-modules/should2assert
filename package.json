{
  "name": "should2assert",
  "version": "1.3.0",
  "description": "A tool to help you migrating from should to assert",
  "main": "index.js",
  "bin": {
    "should2assert": "bin/codemods"
  },
  "scripts": {
    "autod": "autod",
    "lint": "eslint . --ext .js",
    "test": "npm run lint && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "ci": "npm run lint && npm run cov",
    "contributors": "contributors -f plain -o AUTHORS",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/node-modules/should2assert.git"
  },
  "keywords": [
    "codemods",
    "should",
    "assert"
  ],
  "author": "宗羽 <gxcsoccer@126.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/node-modules/should2assert/issues"
  },
  "homepage": "https://github.com/node-modules/should2assert#readme",
  "dependencies": {
    "co": "^4.6.0",
    "debug": "^2.6.3",
    "jscodeshift": "^0.3.30",
    "meow": "^3.7.0",
    "mz-modules": "^1.0.0",
    "runscript": "^1.2.1",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "autod": "^2.8.0",
    "coffee": "^3.3.0",
    "contributors": "^0.5.1",
    "cpy": "^5.0.0",
    "egg-bin": "^3.2.0",
    "egg-ci": "^1.6.0",
    "eslint": "^3.18.0",
    "eslint-config-egg": "^3.2.0",
    "mm": "^2.1.0"
  },
  "snyk": true
}
